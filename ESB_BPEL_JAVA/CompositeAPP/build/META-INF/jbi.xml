<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://enterprise.netbeans.org/bpel/BpelModule/bpelCarro" xmlns:ns2="http://j2ee.netbeans.org/wsdl/BpelModule/Archivos/llamadaCarro" xmlns:ns3="http://enterprise.netbeans.org/bpel/BpelModule/bpelSolicitar" xmlns:ns4="http://j2ee.netbeans.org/wsdl/BpelModule/Archivos/llamadaSolicitar" xmlns:ns5="http://j2ee.netbeans.org/wsdl/BpelModule/Archivos/WSDLCarro" xmlns:ns6="http://j2ee.netbeans.org/wsdl/BpelModule/Archivos/WSDLSolicitar" xmlns:ns7="http://enterprise.netbeans.org/bpel/BpelModule/bpelPiloto" xmlns:ns8="http://j2ee.netbeans.org/wsdl/BpelModule/Archivos/llamadaPiloto" xmlns:ns9="http://j2ee.netbeans.org/wsdl/BpelModule/Archivos/WSDLPiloto" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>CompositeAPP</name>
            <description>Represents the Service Assembly of CompositeAPP</description>
        </identification>
        <service-unit>
            <identification>
                <name>CompositeAPP-BpelModule</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>BpelModule.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>CompositeAPP-sun-rest-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-rest-binding.jar</artifacts-zip>
                <component-name>sun-rest-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="RestOutboundPortTypeRole_partnerRole" service-name="ns1:llamarAuto"/>
                <provider endpoint-name="RestOutboundWSDL_OutboundPort" service-name="ns2:RestOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="RestOutboundPortTypeRole_partnerRole" service-name="ns3:PartnerLink2"/>
                <provider endpoint-name="RestOutboundWSDL_OutboundPort" service-name="ns4:RestOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="RestInboundWSDL_InboundPort" service-name="ns5:RestInboundService"/>
                <provider endpoint-name="RestInboundPortTypeRole_myRole" service-name="ns1:Auto"/>
            </connection>
            <connection>
                <consumer endpoint-name="RestInboundWSDL_InboundPort" service-name="ns6:RestInboundService"/>
                <provider endpoint-name="RestInboundPortTypeRole_myRole" service-name="ns3:PartnerLink1"/>
            </connection>
            <connection>
                <consumer endpoint-name="RestOutboundPortTypeRole_partnerRole" service-name="ns7:salidaAvisoPiloto"/>
                <provider endpoint-name="RestOutboundWSDL_OutboundPort" service-name="ns8:RestOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="RestInboundWSDL_InboundPort" service-name="ns9:RestInboundService"/>
                <provider endpoint-name="RestInboundPortTypeRole_myRole" service-name="ns7:avisoPilotoPL"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
